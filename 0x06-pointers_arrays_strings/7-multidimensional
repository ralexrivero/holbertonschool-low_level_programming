#include "holberton.h"

/**
 * leet - encodes a string into 1337
 * Descripion: encodes usin the leet
 * Return: when done complete
 * @str: string to encode
 */

char *leet(char *s)
{
char *lt[5][3] = {{'a', 'A', '4'}, {'e', 'E', '3'}, {'o', 'O', '0'},
{'t', 'T', '7'}, {'l', 'L', '1'}};
int i, j;

for (i = 0; s[i] != '\0'; i++)
{
for (j = 0; j < 4; j++)
{
	if (s[i] == lt[j][0] && lt[j][1])
	{
	s[i] = lt[j][2];
	}
}
}
return (s);
}
